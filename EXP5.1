//function defination
int factorialIterative(int num)      
{
    int prod=1;
    for(int i=1; i<=num; i++)
    {
       //prod=prod*i
        prod*=1;  
    }
    return prod;
}
int main()
{
    int num;
    
    //Title
    printf("\t\t\t***Iterative Factorial function***\n\n");      
    printf("Enter Number=");
    scanf("%d",&num);
    
    //funtion call
    printf("Factorial of %d is %d",num,factorialIterative(num));  
    return 0;
}
