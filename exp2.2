 #include <stdio.h>
 
 int main(){
    
    int start,end,i,sum;
    
    printf("Sum of all odd numbers between two numbers\n");
   
    printf("Enter starting number\n");
    scanf("%d",&start);
   
    printf("Enter ending number\n");
    scanf("%d",&end);
   
    if (start>end)
    printf("Error: Starting number is larger than ending number!");
    else{
        for(i=start+1;i<end;i++)
        {
            if(i%2!=0)
            sum=sum+i;
        }
        printf("The sum of odd numbers between %d and %d is %d",start,end,sum);
    }
}
